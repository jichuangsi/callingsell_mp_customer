.page-view{padding-bottom: 20rpx;}
/*没有数据时显示*/
.empty-view{ text-align: center; height: 900rpx;}
.empty-box .cuIcon-info{ font-size: 150rpx;}
.empty-box .no-data-text{ margin: 10rpx 0 60rpx 0;}
/*列表*/
.item-view{padding:20rpx;margin:30rpx auto;width:690rpx;background: var(--white); border-radius: 10rpx;}
.store-name{margin-left:10rpx;font-size: 32rpx;color:#333;font-weight: bold;}
.status-text{font-size: 28rpx;}
.goods-view{margin:20rpx 0;}
.good-image{width:120rpx;height: 120rpx;}
.good-info{margin-left:20rpx;}
.info-title{font-size: 28rpx;color:#333;}
.info-spec{margin:4rpx 0 20rpx 0;font-size: 22rpx;color:#999;}
.info-num{font-size: 28rpx;color:#999;}
.goods-cost{padding-bottom:20rpx;text-align: right;border-bottom:1rpx solid #f5f5f5;}
.cost-text{font-size: 28rpx;color:#333;}
.total-cost{font-size: 30rpx;margin-left:30rpx;}
.btn-view{margin-top:30rpx;text-align: right;}
.btn-view navigator,.btn-view button{margin-left:20rpx;}

